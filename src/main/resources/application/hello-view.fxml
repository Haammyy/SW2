<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="400.0" style="-fx-background-color: #a0a0a0;" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <TextField fx:id="userName" prefHeight="26.0" prefWidth="427.0" promptText="Username">
               <VBox.margin>
                  <Insets bottom="10.0" left="150.0" right="150.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="passWord" promptText="Password">
               <VBox.margin>
                  <Insets left="150.0" right="150.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="logInButton" mnemonicParsing="false" onAction="#onLoginClick" text="Login">
               <VBox.margin>
                  <Insets left="250.0" top="10.0" />
               </VBox.margin>
            </Button>
            <HBox prefHeight="146.0" prefWidth="560.0">
               <children>
                  <VBox prefHeight="36.0" prefWidth="130.0">
                     <children>
                        <Label fx:id="welcomeText" text="Logged Out" />
                        <Label fx:id="localeLabel" prefHeight="18.0" prefWidth="137.0" text="LocaleInfo" textFill="RED">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font></Label>
                     </children>
                  </VBox>
                  <Label alignment="CENTER_RIGHT" contentDisplay="BOTTOM" prefHeight="18.0" prefWidth="134.0" text="Label">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <HBox.margin>
                        <Insets left="300.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets top="150.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets top="100.0" />
         </padding>
      </VBox>
   </children>
</VBox>
